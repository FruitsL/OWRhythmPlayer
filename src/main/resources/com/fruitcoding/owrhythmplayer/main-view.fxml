                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label contentDisplay="TOP" text="음악 파일" textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.rowIndex="0">
                     <GridPane.margin>
                        <Insets right="10.0" />
                     </GridPane.margin></Label>
                  <MapSplitMenuButton fx:id="musicSplitMenuButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="None" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="0">
                  </MapSplitMenuButton>
                  <Label text="재생 장치" textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
                     <GridPane.margin>
                        <Insets right="10.0" />
                     </GridPane.margin></Label>
                  <MapSplitMenuButton fx:id="speakerSplitMenuButton1" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="None" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="1"/>
                  <Label text="음량" textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
                     <GridPane.margin>
                        <Insets right="10.0" />
                     </GridPane.margin></Label>
                  <TooltipSlider fx:id="speakerSlider1" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="시작 딜레이 (ms)" textAlignment="RIGHT" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
