                        <Insets />
                     </GridPane.margin></TooltipSlider>
                  <Label text="시작 딜레이 (ms)" textAlignment="RIGHT" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="4">
